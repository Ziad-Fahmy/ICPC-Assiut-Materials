#include <iostream>
using namespace std;
int main()
{
    while(true)
    {
        int n,m;
        cin>>n>>m;

        if(n<=0 || m<=0) break;

        int x=min(n,m);
        int t=max(n,m);
        int sum=0;
        for(int i=x;i<=t;i++)
        {
            cout<<i<<" ";
            sum +=i;
        }
        cout<<"sum ="<<sum<<endl;
    }
}
