#include <iostream>
using namespace std;
int main()
{
    int n;cin>>n;
    int a[n];

    int even=0;
    int odd=0;
    int pos =0;
    int neg=0;

    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        if(a[i]%2==0) even++;else odd++;
        
        if(a[i]>0) pos++;else if(a[i]<0) neg++;
        


    }
    cout<<"Even: "<<even<<endl;
    cout<<"Odd: "<<odd<<endl;
    cout<<"Positive: "<<pos<<endl;
    cout<<"Negative: "<<neg<<endl;
}
